{
  "manifest_version": 1,
  "id": "smaller-compact-mode",
  "name": "Smaller Compact Mode",
  "description": "A modern rebuild of n7itro's Smaller Compact Mode for Zen Browser, tightening toolbar, tab, and menu spacing for a dense layout.",
  "version": "1.0.0",
  "type": "theme",
  "targets": ["desktop"],
  "authors": [
    {
      "name": "n7itro",
      "role": "Original author"
    },
    {
      "name": "Zen Mods Maintainers",
      "role": "Maintainer"
    }
  ],
  "homepage": "https://github.com/bubbabdfjhgldkfhg/zen-mods/blob/main/Smaller%20Compact%20Mode/",
  "license": "MIT",
  "entrypoints": {
    "userChrome": {
      "styles": [
        "styles/compact-chrome.css"
      ]
    },
    "userContent": {
      "styles": [
        "styles/compact-content.css"
      ]
    }
  }
}
